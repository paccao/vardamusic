    "@types/d3-shape": "^3.1.8",
    "@types/node": "^25.2.2",
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "layerchart": "2.0.0-next.43",
    "openapi-typescript": "^7.12.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
