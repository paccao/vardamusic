    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "runed": "^0.37.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@dnd-kit-svelte/svelte": "^0.1.6",
    "@dnd-kit/abstract": "^0.2.4",
    "@dnd-kit/helpers": "^0.2.4",
