    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.3.1"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false,
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "tailwindStylesheet": "./src/app.css"
